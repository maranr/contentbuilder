{
  "name": "contentbuider",
  "private": true,
  "version": "1.0.0",
  "description": "Monorepo for course content builder",
  "main": "index.js",
  "type": "commonjs",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
